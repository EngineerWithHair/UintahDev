
#include <config/sci.cf>

LOCALINCLUDES = -I. -I../modules -I../../wrapper -I../../wrapper/Mt -I../../wrapper/libMInT
OBJS = ColorManager.o  Connection.o  Field3DPort.o  GeometryPort.o  \
	Module.o  Modules.o  ModuleList.o  MotifCallbackBase.o  \
	MUI.o  \
	MtXEventLoop.o \
	Network.o  NetworkEditor.o  Port.o  \
	SoundPort.o \
	UserModule.o  XQColor.o \
	main.o
SRCS = ColorManager.cc Connection.cc Field3DPort.cc GeometryPort.cc \
	Module.cc Modules.cc ModuleList.cc MotifCallbackBase.cc \
	MUI.cc \
	MtXEventLoop.cc \
	Network.cc NetworkEditor.cc Port.cc \
	SoundPort.cc \
	UserModule.cc XQColor.cc \
	main.cc
TARGET = project

SCI_LIBS = \
	../modules/libmodules.a \
	SCI_MultitaskLib \
	SCI_ClassLib \
	../../wrapper/libMInT/libMInT.a

SCI_LDFLAGS = -L../../audio -laudio SCI_LM SCI_LTHREAD SCI_LMOTIF SCI_LX11 -lgen

depend:: XQColorDB.h

SCIDependTarget($(SRCS))
SCIProgramTarget($(TARGET), $(OBJS), $(SCI_LIBS), $(SCI_LDFLAGS))

XQColorDB.h: rgb.txt gendb.awk
	rm -f XQColorDB.h
	awk -f gendb.awk rgb.txt > XQColorDB.h
