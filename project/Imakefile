
#include <config/sci.cf>

LOCALINCLUDES = -I. -I../modules
OBJS = Connection.o  Data.o  Datatype.o  Datatypes.o  \
	Module.o  Modules.o  ModuleList.o  \
	MUI.o  \
	Network.o  NetworkEditor.o  Port.o  Scheduler.o  \
	SoundData.o \
	UserModule.o  \
	main.o
SRCS = Connection.cc Data.cc Datatype.cc Datatypes.cc \
	Module.cc Modules.cc ModuleList.cc \
	MUI.cc \
	Network.cc NetworkEditor.cc Port.cc Scheduler.cc \
	SoundData.cc \
	UserModule.cc \
	main.cc
TARGET = project

SCI_LIBS = \
	../modules/libmodules.a \
	SCI_MultitaskLib \
	SCI_ClassLib

SCI_LDFLAGS = SCI_LM SCI_LTHREAD SCI_LMOTIF SCI_LX11

SCIDependTarget($(SRCS))
