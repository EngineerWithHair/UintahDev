
convert: convertall

all:

#define all convertall
#include <config/sci.cf>

SO_LDFLAGS = -L$(TOP)/lib -lSalmon -lDatatypes -lDataflow -lGeom \
	-lGeometry -lMath -lTCL \
	-lClasslib -lMultitask -lMalloc -lm

#define ConvertProgram(name) @@\
SCIProgramTarget(name, name.o, , $(SO_LDFLAGS)) @@\
					        @@\
SCIDependTarget(name.cc)


ConvertProgram(DENtoSF)
ConvertProgram(ConvMesh)
ConvertProgram(GEtoSF)
ConvertProgram(GenMesh)
ConvertProgram(GenMesh2)
ConvertProgram(GenScalp)
ConvertProgram(JAStoSurf)
ConvertProgram(Mesher)
ConvertProgram(HexMesher)
ConvertProgram(HexUG)
ConvertProgram(ScFldToIves)
ConvertProgram(SegToCS)
ConvertProgram(SurfToJAS)
ConvertProgram(VPtoTriSurf)
ConvertProgram(mbench)
ConvertProgram(convert_geom)
ConvertProgram(benchsolve)
ConvertProgram(matconv)
ConvertProgram(mpbench)
ConvertProgram(ffttest)
ConvertProgram(tecread)
