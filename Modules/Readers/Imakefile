
#define NO_SCI_CC_TARGET

#include <config/sci.cf>

LOCALINCLUDES = -I.. -I../../project

OBJS = ContourSetReader.o  ScalarFieldReader.o  SurfaceReader.o \
	VectorFieldReader.o
SRCS = ContourSetReader.cc ScalarFieldReader.cc SurfaceReader.cc \
	VectorFieldReader.cc
TCLS = ContourSetReader.tcl ScalarFieldReader.tcl SurfaceReader.tcl \
	VectorFieldReader.tcl

.cc.o:
	$(CCC) $(CCFLAGS) -c $<

../modules.o: $(OBJS)
	cd .. && make modules.o

AllTarget(../modules.o)
AllTarget($(TCLS))

SCIDependTarget($(SRCS))

ContourSetReader.h: Reader.h
	sed 's/TYPE/ContourSet/g' < Reader.h > ContourSetReader.h

ContourSetReader.cc: Reader.cc ContourSetReader.h
	sed 's/TYPE/ContourSet/g' < Reader.cc > ContourSetReader.cc

ContourSetReader.tcl: Reader.tcl
	sed 's/TYPE/ContourSet/g' < Reader.tcl > ContourSetReader.tcl

ScalarFieldReader.h: Reader.h
	sed 's/TYPE/ScalarField/g' < Reader.h > ScalarFieldReader.h

ScalarFieldReader.cc: Reader.cc ScalarFieldReader.h
	sed 's/TYPE/ScalarField/g' < Reader.cc > ScalarFieldReader.cc

ScalarFieldReader.tcl: Reader.tcl
	sed 's/TYPE/ScalarField/g' < Reader.tcl > ScalarFieldReader.tcl

SurfaceReader.h: Reader.h
	sed 's/TYPE/Surface/g' < Reader.h > SurfaceReader.h

SurfaceReader.cc: Reader.cc SurfaceReader.h
	sed 's/TYPE/Surface/g' < Reader.cc > SurfaceReader.cc

SurfaceReader.tcl: Reader.tcl
	sed 's/TYPE/Surface/g' < Reader.tcl > SurfaceReader.tcl

VectorFieldReader.h: Reader.h
	sed 's/TYPE/VectorField/g' < Reader.h > VectorFieldReader.h

VectorFieldReader.cc: Reader.cc VectorFieldReader.h
	sed 's/TYPE/VectorField/g' < Reader.cc > VectorFieldReader.cc

VectorFieldReader.tcl: Reader.tcl
	sed 's/TYPE/VectorField/g' < Reader.tcl > VectorFieldReader.tcl

