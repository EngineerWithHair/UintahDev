
#include <config/sci.cf>

SUBDIRS = Contours FEM Fields Matrix Mesh Readers \
	Salmon Sound Surface Visualization


SCISubdirs($(SUBDIRS))

AllTarget(libModules.o)

OBJS = Contours/libContours.o FEM/libFEM.o Matrix/libMatrix.o \
	Mesh/libMesh.o Salmon/libSalmon.o Sound/libSound.o \
	Readers/libReaders.o Visualization/libVisualization.o

libModules.o: $(OBJS)
	ld -r -o libModules.o $(OBJS)

